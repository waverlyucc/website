<wpml-config>
    
    <!-- Total Custom VC Modules/Shortcodes -->
    <shortcodes>
        <shortcode>
            <tag>vcex_shortcode</tag>
        </shortcode>
        <shortcode>
            <tag>vcex_heading</tag>
            <attributes>
                <attribute>text</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>vcex_button</tag>
        </shortcode>
        <shortcode>
            <tag>vcex_leader</tag>
